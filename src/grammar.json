{
  "name": "sclin",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_expression"
      }
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "str"
        },
        {
          "type": "SYMBOL",
          "name": "num"
        },
        {
          "type": "SYMBOL",
          "name": "_cmd"
        }
      ]
    },
    "str": {
      "type": "PATTERN",
      "value": "\"([^\"\\\\]|\\\\.)*\""
    },
    "num": {
      "type": "PATTERN",
      "value": "\\d*\\.\\d+|\\d+"
    },
    "_cmd": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "dot"
        },
        {
          "type": "SYMBOL",
          "name": "hash"
        },
        {
          "type": "SYMBOL",
          "name": "esc"
        },
        {
          "type": "SYMBOL",
          "name": "mstr"
        },
        {
          "type": "SYMBOL",
          "name": "brack"
        },
        {
          "type": "SYMBOL",
          "name": "word"
        },
        {
          "type": "SYMBOL",
          "name": "cmd"
        }
      ]
    },
    "dot": {
      "type": "STRING",
      "value": "."
    },
    "brack": {
      "type": "PATTERN",
      "value": "[(\\[{}\\])]+"
    },
    "hash": {
      "type": "PATTERN",
      "value": "#[^\\s\\d\".]+"
    },
    "esc": {
      "type": "PATTERN",
      "value": "\\\\[^\\s\\d\".]+"
    },
    "mstr": {
      "type": "PATTERN",
      "value": "\"[^\\s\\d\".]+"
    },
    "word": {
      "type": "PATTERN",
      "value": "[A-Za-z]+"
    },
    "cmd": {
      "type": "PATTERN",
      "value": "[^\\s\\d\".A-Za-z]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

